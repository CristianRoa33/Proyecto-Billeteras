---
import Navbar from "../shared/Navbar.astro"
import styles from "../styles/Header.module.css"
import { Image } from "astro:assets"; 
import logo from "@images/Logo-header.webp";


---

<header class={styles.Header_container}>
    <nav class={styles.nav_main_container}>
        <a href="/Proyecto-Billeteras/" class="header-logo">
        <Image  
        src={logo}
        alt="logo"
        loading="eager"
        fetchpriority="high"
        decoding="async"
        class={styles.logo_image}
        >
        </a>

        <div>
        <Navbar />
        </div>

        <div>
            <a href="">
                <img 
                src="/Proyecto-Billeteras/assets/carrito.webp" 
                alt="carrito"
                loading="eager"
                fetchpriority="high"
                decoding="async"
                >
            </a>
        </div>
    </nav>
</header>



